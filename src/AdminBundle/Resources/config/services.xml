<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    
    <services>
        <service id="admin.loadship" class="AdminBundle\Services\LoadShip">
            <argument type="service" id="doctrine" />
            <argument type="service" id="templating" />
            <argument type="service" id="simple_html_dom" />
        </service>
        <service id="admin.loadshipinfoflot" class="AdminBundle\Services\LoadShipInfoflot">
            <argument type="service" id="doctrine" />
            <argument type="service" id="templating" />
            <argument type="service" id="simple_html_dom" />
        </service>
        <service id="admin.loadshipmosturflot" class="AdminBundle\Services\LoadShipMosturflot">
            <argument type="service" id="doctrine" />
            <argument type="service" id="templating" />
            <argument type="service" id="simple_html_dom" />
        </service>		
    </services>
    
</container>
