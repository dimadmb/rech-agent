<?php

namespace BaseBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CruiseSpec
 *
 * @ORM\Table(indexes={@ORM\Index(name="id", columns={"id"})})
 * @ORM\Entity
 */
class CruiseSpec
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(name="burning", type="integer")
     */
    private $burning;




    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set burning
     *
     * @param integer $burning
     * @return CruiseSpec
     */
    public function setBurning($burning)
    {
        $this->burning = $burning;

        return $this;
    }

    /**
     * Get burning
     *
     * @return integer 
     */
    public function getBurning()
    {
        return $this->burning;
    }
}
