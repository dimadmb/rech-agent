class DocumentRepository extends EntityRepository
{
	public function findForSiteMap() {
		$str = "SELECT d.url
			FROM BaseBundle\Entity\Document d
			";
   		$q = $this->_em->createQuery($str);
   		return $q->getResult();
	}
}	